{
	"schemaVersion": 1,
	"id": "hexways",
	"version": "${version}",
	"name": "Hexways",
	"description": "Portal Hex Addon addon for Hex Casting",
	"authors": [
		"Stickia",
		"Thank you too Sam for the Portal Staff",
		"Thank you too ChuijkYahus for the zh_cn translation",
		"And Thanks to Nano for code help"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/hexways/icon.png",
	"environment": "*",
